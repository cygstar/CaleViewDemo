<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CaleViewDemo"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".view.custom.CustomActivity" />
        <activity android:name=".view.full.FullActivity" />
        <activity android:name=".view.simple.SimpleActivity" />

        <activity android:name=".view.colorful.ColorfulActivity" />
        <activity android:name=".view.index.IndexActivity" />
        <activity android:name=".view.meizu.MeizuActivity" />
        <activity android:name=".view.mix.MixActivity" />
        <activity android:name=".view.multi.MultiActivity" />
        <activity android:name=".view.progress.ProgressActivity" />
        <activity android:name=".view.range.RangeActivity" />
        <activity android:name=".view.single.SingleActivity" />
        <activity android:name=".view.solar.SolarActivity" />
        <activity android:name=".view.pager.ViewPagerActivity" />
    </application>

</manifest>